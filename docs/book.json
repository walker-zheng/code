{
    "title": "个人日志",
    "description": "记录工作",
    "author": "walker.zheng<mykulou@gmail.com>",
    "output.name": "site",
    "language": "zh-hans",
    "links": {
        "sidebar": {
            "walker's日志": "http://walker-zheng.github.io/wiki"
        }
    },
    "plugins": [
        "-search",
        "-sharing",
        "-highlight",
        "search-plus",
        "fontsettings",
        "tbfed-pagefooter",
        "splitter",
        "advanced-emoji",
        "emphasize",
        "anchor-navigation-ex",
        "favicon",
        "todo"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "tbfed-pagefooter":{
            "copyright":"Copyright &copy walker.zheng 2017",
            "modify_label":"最近修改:",
            "modify_format":"YYYY-MM-DD HH:mm:ss"
        },
        "favicon": {
            "shortcut": "img/favicon.ico",
            "bookmark": "img/favicon.ico",
            "appleTouch": "img/icon.png",
            "appleTouchMore": {
                "120x120": "img/120.png",
                "180x180": "img/180.png"
            }
        }
    }
}
